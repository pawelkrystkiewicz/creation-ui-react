{
  "name": "@creation-ui/react",
  "version": "14.0.0",
  "description": "React.js UI library.",
  "main": "index.js",
  "author": {
    "name": "Pawe≈Ç Krystkiewicz",
    "email": "pawelkrystkiewicz@outlook.com"
  },
  "scripts": {
    "start": "npx kill-port 3005 && next dev --port 3005",
    "build": "pnpm import-check && next build",
    "preview": "next start",
    "lint": "next lint",
    "nuke": "rm -rf node_modules yarn.lock pnpm-lock.yaml .next && pnpm install",
    "sitemap": "next-sitemap",
    "package:version": "pnpm --filter ./packages/react ci:version",
    "package:build": "pnpm rm:build && pnpm --filter ./packages/react build",
    "package:publish": "pnpm --filter ./packages/react _publish",
    "package": "pnpm package:version && pnpm package:build && pnpm test && pnpm package:publish",
    "package:auto": "pnpm package:build && pnpm test && pnpm package:publish",
    "export": "next export",
    "ci:publish": "pnpm publish -r",
    "ci:version": "git fetch --tags && npm version $(git describe --abbrev=0 --tags)",
    "amend": "git commit --amend --no-edit",
    "import-check": "npx cycle-import-check",
    "rm:build": "rm -rf ./packages/react/build",
    "test": "pnpm package:build && cypress run --component",
    "test:dev": "pnpm package:build && cypress open --component",
    "override-version": "sh ./override-version.sh",
    "generate:docs": "node ./scripts/generate-docs.js",
    "prettify": "prettier --write .",
    "update:tailwind": "pnpm -r add @headlessui/tailwindcss@latest @tailwindcss/aspect-ratio@latest @tailwindcss/typography@latest tailwind-merge@latest tailwindcss-debug-screens@latest prettier-plugin-tailwindcss@latest tailwindcss@latest",
    "init:component": "cd packages/react && ./init-component.sh",
    "knip": "knip"
  },
  "dependencies": {
